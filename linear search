#include <stdio.h>

void main() {
    int a[5],i,flag,search_key;
    printf("enter element");
    for(i=0;i<5;i++)
        scanf("%d",&a[i]);
        printf("enter key to search");
        scanf("%d",&search_key);
        for(i=0;i<5;i++){
            if(search_key==a[i]){
                flag=1;
                break;
            }
            else
                continue;
        }
        
    return 0;
}
